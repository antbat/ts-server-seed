{
    "port": 3000,
    "logging": {
        "level": "debug",
        "debugIndex": "logs",
        "errorIndex": "err-log",
        "serviceName": "seed"
    },
    "mongoDB": {
        "connectionString": "mongodb://localhost/core",
        "collection": {
            "user": "user"
        }
    },
    "elasticSearch": {
        "index": {
            "user": "user"
        },
        "options": {
            "node": "http://localhost:9200",
            "log": "debug",
            "keepAlive": true
        }
    }
}
